$sections:{ 
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" 
           data-parent="#accordion" 
           data-target="#$it.divId$">
        $it.title$
        </a>
      </h4>
    </div>
    <div id="$it.divId$" class="panel-collapse collapse">
    </div>
  </div>
}$